package lalala;

import java.util.Iterator;
import java.util.Set;

import javax.annotation.processing.Processor;

import gigadot.rebound.Rebound;

public class oooo {
   public static void main(String[] args) throws Exception {
      Rebound r = new Rebound("lalala");
      CheckAnnotationParser parser = new CheckAnnotationParser();
      Set<Class<? extends Processor>> classes = r.getSubClassesOf(Processor.class);
      Iterator<Class<? extends Processor>> iter = classes.iterator();
      while (iter.hasNext()) {
          parser.parse(iter.next());
      }


   }
}